{
  "manifest_version": 2,
  "name": "Rickrollify",
  "version": "1.1.2",
  "description": "An extension that grants you a 1/100 chance you get rickrolled on a page",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.11.1.min.js", "background.js"]
    }
  ],

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "icons": {
    "32": "icons/32x32.png",
    "128": "icons/128x128.png"
  }
}
